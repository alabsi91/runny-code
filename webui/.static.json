{
  "ignore": ["dist/**", "backend/**"],

  "build": {
    "minify": true,
    "cleanOutDir": true,
    "analyzeOutput": {
      "enabled": true,
      "deleteUnused": true,
      "exclude": ["sources/assets/icons/*"]
    },
    "convertImages": {
      "enabled": true,
      "quality": 100,
      "preset": "default"
    }
  }
}
